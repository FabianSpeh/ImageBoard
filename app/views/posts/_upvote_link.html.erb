<!-- https://blog.corsego.com/gem-acts-as-votable-2> --->
 
<%= content_tag "div", class:"inline", id: "upvote-#{post.id}" do %>

<%= link_to upvote_post_path(post), method: :patch , remote: true do %>
<% if user_signed_in?%>
<% if current_user.voted_up_on? post%>
    unlike Post
    <% else %>
    like Post
<%end%>
<%end%>
<% end %>
<%end%>
